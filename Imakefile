       EXTRA_INCLUDES = -I. -I./fishmaps
LOCAL_LIBRARIES = $(XLIB)
        DEPLIBS = $(DEPXLIB)
  SYS_LIBRARIES = -lm
    CDEBUGFLAGS = -g

           SRCS = xfish.c medcut.c read.c gifread.c
           OBJS = xfish.o medcut.o read.o gifread.o

ComplexProgramTarget(xfishtank)

xfishy.h: makeh FishList
	./makeh > xfishy.h

xfish.o: xfishy.h

clean::
	$(RM) makeh xfishy.h

