 EXTRA_INCLUDES = -I./fishmaps
LOCAL_LIBRARIES = $(XLIB)
        DEPLIBS = $(DEPXLIB)
  SYS_LIBRARIES = -lImlib2 -lm
   MANSRCSUFFIX = 1x

           SRCS = xfish.c medcut.c
